{{- $.Scratch.Add "index" slice -}}
{{- range where .Site.RegularPages "Type" "in" (slice "posts") -}}
  {{- $.Scratch.Add "index" (dict "title" .Title "content" .Plain "summary" .Summary "categories" .Params.categories "url" .Permalink "date" .Date) -}}
{{- end -}}
{{- $.Scratch.Get "index" | jsonify -}}
